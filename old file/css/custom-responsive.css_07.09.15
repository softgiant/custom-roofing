
@media (max-width: 1171px) {

/* Structure */
.wrappar, .fixed-row { width:96%; margin:0 auto; }	
.color-box-w {  width:49.5%; float:left; }
/*sec nav style*/
.sec_nav ul li a {
    color: #fff;
    display: inline-block;
    font-size: 10px;
    height: 114px;
    line-height: 14px;
    text-decoration: none; width:100%;
}
.gap_toph{ width:30%; float:left;}

/*12-11-14*/
.sec_nav ul li a:link, .navigation ul li a:visited {
    /*background-color: #bd0000;*/
    background-repeat: no-repeat;
    border-bottom: 0 solid #ffffff;
    color: #ffffff;
    display: block;
    height: 101px;
    position: relative;
    text-decoration: none;
    width: 100%;
}

}


