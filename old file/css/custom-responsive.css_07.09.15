.sec_nav ul li a.colr6{ background:#cd0066 url(../images/icon_img6.png) no-repeat center 6px; padding-top:0px;}
.sec_nav ul li a.colr6:hover{ background:#cd0066;  box-shadow:none;  }
.sec_nav ul li a.colr7{ background:#ff690b url(../images/icon_img7.png) no-repeat center 12px; padding-top:0px;}
.sec_nav ul li a.colr7:hover{ background:#ff690b;  box-shadow:none;}
.sec_nav ul li a.colr8{ background:#3e403f url(../images/icon_img8.png) no-repeat center 12px; padding-top:0px;}
.sec_nav ul li a.colr8:hover{ background:#3e403f;  box-shadow:none;}
.sec_nav ul li a{ font-size:10px; color:#fff; line-height:14px; display:block; text-decoration:none; height:78px; position:relative; }

.sec_nav ul li a:link, .navigation ul li a:visited {
	  width:100%;
	  height:78px;
	  background-position:center center !important;
	  
	}
	.sec_nav ul li.seven a:link, .navigation ul li.seven a:visited {
	  width:299px;
	  
	}
	.sec_nav ul li a span {
	  display:none;
	}
	.sec_nav ul li a:hover span {
	  display:table-cell;	  	  
	  position:relative;
	  bottom:0;
	  width:279px;
	  height:78px;
	  vertical-align:middle;
	  padding:0 10px;
	  opacity:0.9;	  
	}
.sec_nav ul li a.active span {
	  display:table-cell;	  	  
	  position:relative;
	  bottom:0;
	  width:279px;
	  height:78px;
	  vertical-align:middle;
	  padding:0 10px;
	  opacity:0.9; box-shadow:none;	  
	}
	.sec_nav ul li.seven a:hover span {
	  width:279px;
	}
