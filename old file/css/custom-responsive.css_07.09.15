    padding: 0 10px;
    position: relative;
    vertical-align: middle;
    width: 354px !important; font-size:14px;
}

.sec_nav {
    float: left;
    margin-bottom: 16px;
    width: 100%; margin-left:12px;
}
.sec_nav ul li a.colr1:hover{ background:#454e80 !important; box-shadow:none;}
.sec_nav ul li a.colr2{ background:#05ad81 url(../images/icon_img2.png) no-repeat center 7px; padding-top:0px;}
.sec_nav ul li a.colr2:hover{ background:#05ad81 !important;  box-shadow:none;}
.sec_nav ul li a.colr3{ background:#68156d url(../images/icon_img3.png) no-repeat center 7px; padding-top:0px;}
.sec_nav ul li a.colr3:hover{ background:#68156d;  box-shadow:none;}
.sec_nav ul li a.colr4{ background:#be0303 url(../images/icon_img4.png) no-repeat center 7px; padding-top:0px;}
.sec_nav ul li a.colr4:hover{ background:#be0303;  box-shadow:none; }
.sec_nav ul li a.colr5{ background:#175f9c url(../images/icon_img5.png) no-repeat center 10px; padding-top:0px;}
